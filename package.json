{
  "name": "api",
  "version": "1.0.0",
  "description": "An API server using Hono, Cloudflare Workers, D1 and KV.",
  "private": true,
  "type": "module",
  "cloudflare": {
    "label": "Worker API Server",
    "products": [
      "Workers",
      "Node.js",
      "D1",
      "KV"
    ],
    "categories": [
      "api",
      "storage"
    ],
    "docs_url": "https://developers.cloudflare.com/workers/runtime-apis/nodejs/http/",
    "preview_icon_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/6c63cca8-354a-4951-45a0-d58769ac6f00/public",
    "preview_image_url": "https://imagedelivery.net/wSMYJvS3Xw-n339CbDyDIA/aa6158f1-7711-4485-f134-a8c802c0a800/preview",
    "publish": true
  },
  "devDependencies": {
    "@cloudflare/vitest-pool-workers": "0.10.0",
    "hono": "4.10.3",
    "typescript": "5.9.3",
    "uuidv7": "1.0.2",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4",
    "wrangler": "4.45.0"
  },
  "scripts": {
    "typegen": "wrangler types",
    "check": "tsc && wrangler deploy --dry-run",
    "deploy": "wrangler deploy",
    "dev": "wrangler dev",
    "build": "wrangler build",
    "predeploy": "wrangler d1 migrations apply d1-api --remote",
    "seed": "wrangler d1 migrations apply d1-api --local",
    "test": "vitest run"
  }
}
