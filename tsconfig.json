{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext"],
    "module": "esnext",
    "moduleResolution": "bundler",
    "noEmit": true,
    "isolatedModules": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "strict": true,
    "types": ["./worker-configuration.d.ts"],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["worker-configuration.d.ts", "src/**/*.ts", "test/**/*.ts"]
}
